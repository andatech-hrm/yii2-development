{
    "name": "culture-pn-psu/yii2-development",
    "description": "Yii 2 for Personnel Development",
    "type": "yii2-extension",
    "keywords": ["yii2","extension"],
    "license": "GPL-3.0",
    "minimum-stability": "stable",
    "support": {
        "source": "https://github.com/culture-pn-psu/yii2-development.git",
        "issues": "https://github.com/culture-pn-psu/yii2-development/issues"
    },
    "authors": [
        {
            "name": "Ahamad Jehdu",
            "email": "ahamad.j@psu.ac.th",
            "role" : "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "culture-pn-psu/yii2-core":"*"
    },
    "autoload": {
        "psr-4": {
